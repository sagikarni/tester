  "version": "0.1.0",
  "private": true,
  "scripts": {
    "watch": "vue-cli-service serve",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "nprogress": "^0.2.0",
    "@mdi/font": "^3.6.95",
    "material-design-icons-iconfont": "^5.0.1",
    "@vuex-orm/core": "^0.31.12",
    "graphql-tag": "^2.10.1",
    "normalizr": "^3.3.0",
    "articulation-phrase-activity": "^0.0.0",
    "articulation-sentense-activity": "^0.0.0",
    "articulation-word-activity": "^0.0.0",
    "categorization-activity": "^0.0.0",
    "commonality-activity": "^0.0.0",
    "facts-activity": "^0.0.0",
    "good-story-activity": "^0.0.0",
    "howto-make-activity": "^0.0.0",
    "meaning-activity": "^0.0.0",
    "memory-cards-activity": "^0.0.0",
    "photo-assembly-activity": "^0.0.0",
    "plain-activity": "^0.0.0",
    "sound-of-life-photo-activity": "^0.0.0",
    "sound-of-life-sound-activity": "^0.0.0",
    "spot-the-difference-activity": "^0.0.0",
    "wh-questions-activity": "^0.0.0",
    "what-is-wrong-activity": "^0.0.0",
    "whats-in-the-picture-activity": "^0.0.0",
    "zoom-activity": "^0.0.0",
    "pricing": "0.0.0",
    "event-source-polyfill": "^0.0.12",
    "babel-polyfill": "^6.26.0",
    "vuex-router-sync": "^5.0.0",
    "tera-core": "^0.0.0",
    "auth-vue": "^0.0.0",
    "activity": "^0.0.0",
    "vuex-store": "0.0.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "style-loader": "^0.23.1",
    "css-loader": "^2.1.1",
    "vuex-module-decorators": "^0.9.8",
    "vuetify": "^1.5.7",
    "register-service-worker": "^1.6.2",
    "vue": "^2.6.6",
    "vue-class-component": "^6.0.0",
    "vue-property-decorator": "^8.0.0",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@types/jest": "^23.1.4",
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-e2e-cypress": "^3.5.0",
    "@vue/cli-plugin-pwa": "^3.5.0",
    "@vue/cli-plugin-typescript": "^3.5.0",
    "@vue/cli-plugin-unit-jest": "^3.5.0",
    "@vue/cli-service": "^3.5.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "lint-staged": "^13.1.0",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.1.0",
    "ts-jest": "^23.0.0",
    "typescript": "^3.2.1",
    "vue-template-compiler": "^2.5.21"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue",
      "ts",
      "tsx"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.tsx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/",
    "globals": {
      "ts-jest": {
        "babelConfig": true
      }
    }
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.ts": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
