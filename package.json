{
  "name": "tera",
  "version": "0.1.0",
  "private": true,
  "nodemonConfig": {
    "ignore": [
      "public/*"
    ]
  },
  "scripts": {
    "start": "npm run start:server && npm run start:client",
    "watch": "concurrently -p \"[{name}]\" -n \"Database,Server,Client\" -c \"blue.bold,yellow.bold,cyan.bold\" \"npm run db\" \"npm run watch:server\" \"npm run watch:client\"",
    "build": "npm run build:client && npm run build:server -- --mode production",
    "test": "jest --forceExit --coverage --verbose --config jest.config.js",
    "start:qa": "cross-env DEBUG_ENV=QA npm start",
    "start:stage": "cross-env DEBUG_ENV=STAGE npm start",
    "start:prod": "cross-env DEBUG_ENV=PROD npm start",
    "watch:qa": "cross-env DEBUG_ENV=QA npm run watch",
    "watch:stage": "cross-env DEBUG_ENV=STAGE npm run watch",
    "watch:prod": "cross-env DEBUG_ENV=PROD npm run watch",
    "build:prod": "npm run build",
    "start:client": "vue-cli-service serve",
    "watch:client": "vue-cli-service serve",
    "build:client": "vue-cli-service build",
    "start:server": "npm run build:server && node --inspect dist/www.js",
    "watch:server": "concurrently -p \"[{name}]\" -n \"Database,Watch,NodeJS\" -c \"blue.bold,yellow.bold,cyan.bold\" \"npm run db\" \"npm run build:server -- --watch\" \"nodemon --inspect dist/www.js\"",
    "build:server": "npm run webpack -- --config ./server.config.js --mode development",
    "clean:dist": "npm run rimraf -- dist",
    "watch-test": "npm run test -- --watchAll",
    "rimraf": "rimraf",
    "webpack": "webpack",
    "webpack-dev-server": "webpack-dev-server",
    "security": "nsp check",
    "db": "mongod",
    "debug": "node --inspect-brk ./node_modules/@vue/cli-service/bin/vue-cli-service.js build",
    "map": "source-map-explorer ./dist/www.js ./dist/www.js.map",
    "tslint:fix:server": "tslint --fix -c ./apps/server/tslint.json './apps/server/**/*{.ts,.tsx}'",
    "deploy": "npm run build:server && winscp.com /script=deploy",
    "deploy:server": "npm run build:server && winscp.com /script=deploy-server"
  },
  "dependencies": {
    "@passport-next/passport-facebook": "^3.0.0",
    "@sendgrid/mail": "^6.3.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.4",
    "@types/gsap": "^1.19.2",
    "@types/handlebars": "^4.0.39",
    "@types/lodash": "^4.14.116",
    "@types/node": "^10.11.3",
    "@types/nprogress": "0.0.29",
    "awesome-typescript-loader": "^5.2.1",
    "bcryptjs": "^2.4.3",
    "concurrently": "^4.0.1",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "cross-env": "^5.2.0",
    "current-device": "^0.7.8",
    "dotenv": "^6.0.0",
    "express-conditional-middleware": "^2.1.1",
    "express-session": "^1.15.6",
    "gsap": "^2.0.2",
    "helmet": "^3.13.0",
    "json-loader": "^0.5.7",
    "jsonwebtoken": "^8.3.0",
    "material-design-icons-iconfont": "^3.0.3",
    "method-override": "^3.0.0",
    "mongoose": "^5.3.0",
    "node-mocks-http": "^1.7.0",
    "node-uuid": "^1.4.8",
    "nosleep.js": "^0.7.0",
    "nprogress": "^0.2.0",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth20": "^1.0.0",
    "passport-linkedin": "^1.0.0",
    "passport-twitter": "^1.0.4",
    "randomstring": "^1.1.5",
    "register-service-worker": "^1.0.0",
    "secure-ls": "^1.2.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "typescript-collections": "^1.3.2",
    "vue": "^2.5.17",
    "vue-awesome-swiper": "^3.1.3",
    "vue-class-component": "^6.0.0",
    "vue-i18n": "^8.1.0",
    "vue-property-decorator": "^7.0.0",
    "vue-resource": "^1.5.1",
    "vue-router": "^3.0.1",
    "vuetify": "^1.2.5",
    "vuex": "^3.0.1",
    "vuex-class": "^0.3.1",
    "vuex-router-sync": "^5.0.0",
    "webpack-node-externals": "^1.7.2",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@types/jest": "^23.1.4",
    "@types/mongoose": "^5.2.17",
    "@vue/cli-plugin-e2e-nightwatch": "^3.0.4",
    "@vue/cli-plugin-pwa": "^3.0.4",
    "@vue/cli-plugin-typescript": "^3.0.4",
    "@vue/cli-plugin-unit-jest": "^3.0.4",
    "@vue/cli-service": "^3.0.4",
    "@vue/test-utils": "^1.0.0-beta.20",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.1",
    "ts-jest": "^23.0.0",
    "tslint-config-airbnb": "^5.11.0",
    "typescript": "^3.0.0",
    "vue-template-compiler": "^2.5.17",
    "webpack-cli": "^3.1.2"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue",
      "ts",
      "tsx"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/"
  },
  "author": "Shlomi Levi (wizardnet972@gmail.com)",
  "license": "MIT"
}
